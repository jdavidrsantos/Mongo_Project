lass('active');\n            // [].forEach.call(browsers, function(el) {\n            var text = input.value.toUpperCase();\n            var result = new Array()\n            HTMLCollection.prototype[Symbol.iterator] = Array.prototype[Symbol.iterator];\n            for (let option of browsers) {\n            if(option.outerText.toUpperCase().indexOf(text) > -1){\n\n              console.log('option.outerText',browsers)\n              option.style.display = \"block\";\n            }else{\n            option.style.display = \"none\";\n                }\n            result.push(option.outerText.toUpperCase())\n            } }\n\n          input.onkeydown = function(e) {\n          if(currentFocus !=-1 || currentFocus === 0 || currentFocus === -1 ){\n          $(browsers).removeClass('active');\n           }\n           console.log('input.onkeydown')\n\n\n          if(e.keyCode == 40){\n          currentFocus++\n          $(browsers[currentFocus]).addClass('active');\n             }\n           else if(e.keyCode == 38){\n           currentFocus--\n           $(browsers[currentFocus]).addClass('active');\n          }\n               else if(e.keyCode == 13){\n                console.log('soy e', e.preventDefault())\n                console.log('currentFocus total',currentFocus)\n               e.preventDefault();\n                   if (currentFocus > -1) {\n                    console.log('click')\n                   browsers[currentFocus].click();\n                   browsers[currentFocus].click();\n                  }\n             }\n           }\n\n          // function addActive(x) {\n          //     if (!x) return false;\n          //     removeActive(x);\n          //     if (currentFocus >= x.length) currentFocus = 0;\n          //     if (currentFocus < 0) currentFocus = (x.length - 1);\n          //     x[currentFocus].classList.add(\"active\");\n          //   }\n          //   function removeActive(x) {\n          //     for (var i = 0; i < x.length; i++) {\n          //       x[i].classList.remove(\"active\");\n          //     }\n          //   }\n            }     \n\n         } //End methods\n}//export default End\n\n</script>\n\n\n<style lang=\"scss\" scoped>\n\n.form-select{\nwidth: 20%;\ndisplay: initial;\n}\n\n.noBorderTable td, th {\n  border-style: none;\n}\n\n.modal-footer {\n  border-top:none\n}\n\n\n.thinverse{\n  font-weight: normal;\n  width: 55rem;\n}\n\n.totalPriceCartText{\n  max-width: 2rem;\n  min-width: 2rem;\n}\n\n.totalPriceCart{\nfont-weight: bold;\nfont-size: 2rem;\n}\n\n.removeIcon{\ncolor: red;\nzoom: 1.5;\ndisplay: flex;\nposition: absolute;\n}\n\n.is-hover {\n  --bs-table-hover-color: #ff0000 !important;\n}\n\n.emptyCar{\n  zoom: 5;\n  margin: 0.5rem;\n}\n\n.totalPrice{\n    text-align: end;\n}\n\ninput[type=number] {\n        /*for absolutely positioning spinners*/\n        position: relative; \n        padding: 5px;\n        padding-right: 25px;\n      }\n\n      input[type=number]::-webkit-inner-spin-button,\n      input[type=number]::-webkit-outer-spin-button {\n        opacity: 1;\n      }\n\n      input[type=number]::-webkit-outer-spin-button, \n      input[type=number]::-webkit-inner-spin-button {\n        -webkit-appearance: inner-spin-button !important;\n        width: 25px;\n        position: absolute;\n        top: 0;\n        right: 0;\n        height: 100%;\n      }\n\n.titlewidth{\n    width: 40rem;\n}\n.priceProduct{\n    font-size: 1.3rem;\n    width: 8rem;\n}\n.table{\n    vertical-align: initial;\n}\n.productsImage{\n    width:  70px;\n    height: 90px;\n    overflow: hidden;\n    --bs-table-hover-bg: none\n}\n.direct{\n    color: white;\n}\n.car{\n    color: #49a7ff;\n    zoom: 1.3;\n    margin: 5px;\n}\n.carAdded{\n    font-size: 10px;\n    top: 10%;\n    position: absolute;\n    color: white;\n}\n\n\n///datalist\n/// \n.inputCity2{\ndisplay: block;\nborder-radius: 5px 5px 0 0;\nheight: 2.5rem;\nposition: relative;\noverflow: hidden;\n}\n\n.city{\n  position: absolute;\n  overflow-x: auto;\n  max-block-size: -webkit-fill-available;\n  max-height: 10rem;\n  display: list-item;\n  margin-left: 37rem;\n}\n\nfieldset {\n  border: 1px solid blue;\n  width: 360px;\n  border-radius: 5px;\n}\n\n// legend, label{\n//   color: blue; \n//   font-size: 24px;\n//   font-family: sans-serif;\n// }\n\ndatalist {\n  position: absolute;\n  background-color: white;\n  border: 1px solid blue;\n  border-radius: 0 0 15px 15px;\n  border-top: none;\n  font-family: sans-serif;\n  width: 270px;\n  padding: 5px;\n  overflow-y: auto;\n  margin-left: 0.4rem;\n}\n\noption {\n  //  background-color: white;\n  padding: 4px;\n  color: rgb(0, 0, 0);\n  margin-bottom: 1px;\n   font-size: 18px;\n  cursor: pointer;\n}\n\noption:hover,  .active{\n  // background-color: lightblue;\n  background-color: #e4dffb\n}\n\ndatalist:hover{\n  background-color: #e4dffb\n}\n// datalist:hover{\n//   background-color: red;\n// }\n\n// .noBG{\n//   background-color: none\n// }\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}