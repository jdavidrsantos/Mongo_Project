  \n        this.articles.map((image, index) => {\n        let imagen =  image.image\n        console.log('soy imagen', imagen)  })         \n  \n        console.log($event)\n\n        if(this.ZoominProducts === true){\n            this.ZoominProducts = false\n        } if(this.ZoominProducts === false){\n            this.ZoominProducts = true\n            $(\"#exampleModal\").modal('show');\n\n\n        \n\n\n\n                             }\n\n        },\n     \n    \n\n\n    isValidDescription(description){\n    return description.length > 50 ? description.substring(0, 100) + '...': description\n    },\n\n// get(){ return this.$store.getters['products/articles'], console.log('lo llamo, getter')},\n\n\nloadproducts(){\n         this.$store.dispatch ('products/loadproducts')\n                        },\n\n    // mounting(){\n    //     this.articles.description === 'hi' ? this.articles : this.articles\n    //     console.log('yes david',this. descriptionProducts)\n    //         },\n\n\ndescriptions (){\n    let result= []\n    //start \n    this.articles.map((descrip, index) => {\n    let descriptionss =  { id : index,\n                        description: descrip.description\n                        }\n    if (descriptionss.description==='hi' || descriptionss.description.length > 4){\n        setTimeout(() => {\n            this.articles.map((descrip, index) => {\n            let descriptions =  { id : index,\n                                  description: descrip.description\n                                }\n    if(descriptions.description.length > 5 && descriptions.id === index){\n                    result.push( {\n                    description: descriptions.description.substring(0, 100) + '...',\n                    id : index\n                                } )\n        this. descriptionProducts = result\n                    }else{\n                    this.descriptionProducts = descriptions.description\n                            }\n                    })  //end\n                                        },600)\n                                            } \n     })  //end\n\n} //description method end\n    }, //method end\n} //export default end\n\n\n</script>\n\n\n<style lang=\"scss\" scoped>\n\nthead, tbody, tfoot, tr, td, th{\n    border-style: none;\n}\n\n.discount-content:hover .icons {\nbackground-color: whitesmoke;\nbottom: 0;\n    left: 0;\n    right: 0;\n    display: flex;\n    justify-content: center;\n    position: absolute;\n    margin: 35%;\n\n};\n\n.icons{\nbottom: 0;\nleft: 0;\nright: 0;\ndisplay: flex;\njustify-content: center;\nposition: absolute;\nmargin: 35%;\ndisplay: none;\n}\n\n.icon{\n    margin: 10px;\n    zoom: 150%;\n}\n\n.icons:hover{\n    background-color: orange;\n}\n\n.icon:hover{\n    color: red;\n}\n\n.table{\n width: 33%;\n\n}\n\n.productsImage{\n    width:  200px;\n    height: 300px;\n    overflow: hidden;\n}\n\n\n.productsImage img{\n height: auto;\n width:  200px;\n}\n.aligning{\n    height: 6rem;\n    justify-content: center;\n}\n.boldPrice{\n    font-weight: bold;\n}\n\n.discount-content{\n    position: relative;\n}\n\n.discount{\n    position: absolute;\n    top: 0px;\n    background-color: black;\n    border-radius: 3px;\n    color: #fff;\n    font-weight: 700;  \n}\n\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}