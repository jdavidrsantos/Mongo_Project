el\" aria-hidden=\"true\" >\n  <div class=\"modal-dialog modal-xl\">\n    <div class=\"modal-content\">\n      <div class=\"modal-header\">\n        <h1 class=\"modal-title fs-5 center\" id=\"exampleModalLabel\">CART</h1>\n        <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\n      </div>\n      <div class=\"modal-body text-center\">\n    <div v-if=\"carShop.length !== 0\">\n        <table class=\"table table-hover\" v-for=\"(products, index) in carShop\" :key=\"index\" >\n  <thead>\n    <tr>\n    </tr>\n  </thead>\n  <tbody class=\"text-center\">\n    <tr>\n      <img class=\"productsImage\" :src=\"`${products.image}`\" alt=\"\">\n      <td @mouseenter=\"ishover\" @mouseleave=\"ishoverno\" class=\"pointer\"> <span @click=\"deleteProduct(products.id)\" class=\"text-center hoverProduct\"><i class='fa fa-remove removeIcon'></i> Remove</span></td>\n      <td class=\"titlewidth\">{{products.title}}</td>\n      <td><input type=\"number\" onchange=\"this.value = Math.abs(this.value)\" required oninput=\"validity.valid||(value='')\" min=\"1\" max=\"9\" step=\"1\" v-model.number=\"products.quantity\" ></td>\n      <td class=\"priceProduct\">$ {{Math.abs((products.price*products.quantity)).toFixed(2)}}</td>\n    </tr>\n  </tbody> \n        </table>\n\n<div class=\"delivery\">\n        <h1 class=\"general inputHidden pb-2\">Delivery Order Form</h1>\n        \n        <div class=\"totalPrice\">\n        <table class=\"table\">\n              <tbody class=\"noBorderTable\">\n                    <tr>\n                      <td class=\"fillSpace general1 inputHidden1\">\n                        <label>\n                          <span class=\"labelClass\">Barrio:</span>   \n                          <input type=\"text\" class=\"inputClass\" style=\"margin-right:1rem\">\n\n                          <select class=\"form-select CitiesDepartments inputClass\" aria-label=\".form-select-lg\">\n                            <option selected>Selecciona Departamento</option>\n                            <option value=\"1\">One</option>\n                            <option value=\"2\">Two</option>\n                            <option value=\"3\">Three</option>\n                          </select>\n\n                        <input type=\"text\" id=\"deparmentList\" class=\"inputClass depart\" >\n                        </label>\n                      </td>\n                      <td scope=\"row\" class=\"thinverse\">\n                        <form action=\"\">\n                          <label for=\"colombiaCities2\"> <span style=\"margin-right: 1rem; width: 8.5rem;\">Shipping to: </span>\n                            <div style=\"width: 100%\" autocomplete=\"off\">\n                            <VueSingleSelect @click=\"fillDeparmentInput()\" @change=\"fillDeparmentInput()\" v-on:keyup.enter=\"fillDeparmentInput()\" \n                                      :options= this.cityColombia \n                                      :value=this.cityColombia \n                                      placeholder=\"Select your city\"\n                                      :maxResults= this.general.length\n                                      maxHeight=\"180px\"\n                                      inputId=\"listCities\"\n                                      :getOptionValue='(a) => {return city = a}'            \n                              >\n                              </VueSingleSelect>\n                            </div>\n                          </label> \n                        </form>\n                      </td>\n                      <td style=\"padding-right: 1rem\"><span>$ pending</span>\n                      </td>\n                    </tr>\n\n                    <tr>\n                    <td colspan=\"1\" class=\"general inputHidden\">  \n                        <label>\n                          <div style=\"margin-left:1rem\">\n                          <VueSingleSelect\n                                      :options=this.tipeCalle\n                                      :required=\"true\"\n                                      :value=this.tipeCalle\n                                      placeholder=\"Tipo de calle\"\n                                      :maxResults= this.tipeCalle.length\n                                      maxHeight=\"181px\"\n                                      inputId=\"none\"\n                              >\n                              </VueSingleSelect>\n                            </div>\n                        <label> <span style=\"margin-right: -0.5rem; margin-left: 2rem;\">Calle:</span>   \n                          <input type=\"text\" class=\"inputClass\">\n                        </label>\n                      </label>  \n                    </td>\n                    <td class=\"general inputHidden\">    \n                    <label style=\"width: 28rem;\">\n                      <span style=\"margin-left: 1rem;\">Numero:</span>   \n                          <input style=\"width: 9.8rem\" type=\"text\" class=\"inputClass\">\n\n                        <span style=\"margin-left: 1rem;\">-</span>   \n                          <input style=\"width: 9.8rem\" type=\"text\" class=\"inputClass\">\n                        </label>\n                    </td>\n                    </tr>\n\n                    <tr>\n                      <td class=\"general inputHidden\">\n                        <span style=\"margin-left: 1rem;float:left\">Nombre y apellido</span>   \n                          <input style=\"width: 100%\" type=\"text\" class=\"inputClass\">\n                      </td>\n\n                      <td class=\"general inputHidden\">\n                        <span style=\"margin-left: 1rem;float:left\">Telefono de contacto</span>   \n                          <input style=\"width: 100%\" type=\"text\" class=\"inputClass\">\n                      </td>\n                    </tr>\n\n                    <tr class=\" border-1 border-bottom border-danger mb-4\">\n                      <td colspan=\"2\" class=\"general inputHidden\">\n                        <span style=\"margin-left: 1rem;float:left\">Referencias adicionales para las entregas en esta dirección</span>   \n                          <input style=\"width: 100%; height: 70px; margin-bottom: 30px\" type=\"text\" class=\"inputClass\" placeholder=\"Descripción de la fachada, puntos de referencia para encontrarla, indicaciones de seguridad, etc.\" maxlength=\"128\" autocomplete=\"do-not-autofill\">\n                      </td>\n                    </tr>\n\n                 \n               </tbody>\n        </table>\n        </div>\n</div>\n        <table class=\"table totalPrice\">\n          <tr class=\"totalPriceCart\">\n                      <td></td>\n                      <td scope=\"row\"><span>Total</span></td>\n                      <td class=\"totalPriceCartText\"><span>$  {{Math.abs(totalPriceCart)}}</span></td>\n           </tr>\n        </table>\n\n    </div>\n        <div v-else>\n         <h1>Empty cart</h1>\n         <i class=\"fas fa-shopping-cart emptyCar\"></i>\n        </div>\n      </div>\n      <div v-if=\"carShop.length !== 0\" class=\"modal-footer\">\n        <button @click=\"zeroValidator\" type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">Purchase</button>\n      </div>\n      <div v-else class=\"text-center\">\n        <button type=\"button\" class=\"btn btn-secondary m-3\" data-bs-dismiss=\"modal\">See products</button>\n      </div>\n    </div>\n  </div>\n</div>\n    </div>\n</template>\n<script>\n\n\nimport { defineAsyncComponent } from 'vue'\nimport json from '../helper/co.json'\nimport {mapState, mapActions, mapGetters } from 'vuex';\nimport VueSingleSelect from \"vue-single-select\";\nimport Swal from 'sweetalert2'\n\nexport default {\n\n\n    data: () => ({\n      colombia: json,\n      indexselector:0,\n      city:null,\n      cityColombia:[],\n      general:[],\n   